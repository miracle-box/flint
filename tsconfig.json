{
    "extends": "astro/tsconfigs/strictest",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~/*": ["src/*"],
            "@lib/*": ["src/lib/*"],
            "@custom/*": ["src/custom/*"]
